<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" />
    </sectionGroup>
    <section name="ShelfConfiguration" type="Topshelf.Shelving.ShelfConfiguration, TopShelf"/>
  </configSections>

  <ShelfConfiguration Bootstrapper="Topshelf.Specs.TestAppDomainBootsrapper, TopShelf.Specs"/>
  <startup>
    <supportedRuntime version="v2.0.50727"/>
  </startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="nunit.framework" publicKeyToken="96d09a1eb7f44a77"/>
				<bindingRedirect oldVersion="2.5.8.10295" newVersion="2.5.9.10348"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>

  <common>
    <logging>
      <factoryAdapter type="Common.Logging.Log4Net.Log4NetLoggerFactoryAdapter, Common.Logging.Log4net">
        <arg key="configType" value="FILE-WATCH" />
        <arg key="configFile" value="~\test.log4net.xml" />
      </factoryAdapter>
    </logging>
  </common>
  
  
</configuration>
